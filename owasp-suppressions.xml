<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Suppress false positives and acceptable vulnerabilities -->
    
    <!-- Spring Boot Starter Dependencies - False Positives -->
    <suppress>
        <notes><![CDATA[
        Suppress false positive for Spring Boot starter dependencies
        These are configuration modules, not runtime vulnerabilities
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/spring-boot-starter.*$</packageUrl>
        <cve>CVE-2016-1000027</cve>
    </suppress>
    
    <!-- H2 Database - Test Scope Only -->
    <suppress>
        <notes><![CDATA[
        H2 database is only used in test scope and development
        Not used in production environment
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.h2database/h2.*$</packageUrl>
        <scope>test</scope>
    </suppress>
    
    <!-- Jackson Databind - False Positives -->
    <suppress>
        <notes><![CDATA[
        Suppress false positive for Jackson databind
        These CVEs are not applicable to our usage patterns
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.fasterxml\.jackson\.core/jackson-databind.*$</packageUrl>
        <cve>CVE-2020-36518</cve>
    </suppress>
    
    <suppress>
        <notes><![CDATA[
        Suppress Jackson databind CVE-2020-25649
        Not applicable to our usage pattern
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.fasterxml\.jackson\.core/jackson-databind.*$</packageUrl>
        <cve>CVE-2020-25649</cve>
    </suppress>
    
    <!-- JUnit - Test Scope Only -->
    <suppress>
        <notes><![CDATA[
        JUnit is only used in test scope
        Not included in production builds
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.junit\.jupiter/junit-jupiter.*$</packageUrl>
        <scope>test</scope>
    </suppress>
    
    <suppress>
        <notes><![CDATA[
        JUnit Platform is only used in test scope
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.junit\.platform/junit-platform.*$</packageUrl>
        <scope>test</scope>
    </suppress>
    
    <!-- Mockito - Test Scope Only -->
    <suppress>
        <notes><![CDATA[
        Mockito is only used in test scope
        Not included in production builds
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.mockito/mockito.*$</packageUrl>
        <scope>test</scope>
    </suppress>
    
    <!-- Spring Security - False Positives -->
    <suppress>
        <notes><![CDATA[
        Suppress Spring Security false positives
        These are configuration issues, not runtime vulnerabilities
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.security/spring-security.*$</packageUrl>
        <cve>CVE-2018-1258</cve>
    </suppress>
    
    <!-- Spring Framework - False Positives -->
    <suppress>
        <notes><![CDATA[
        Suppress Spring Framework false positives
        These CVEs are not applicable to our usage
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework/spring-.*$</packageUrl>
        <cve>CVE-2018-1275</cve>
    </suppress>
    
    <!-- Logback - False Positives -->
    <suppress>
        <notes><![CDATA[
        Suppress Logback false positives
        These are configuration issues, not runtime vulnerabilities
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/ch\.qos\.logback/logback.*$</packageUrl>
        <cve>CVE-2017-5929</cve>
    </suppress>
    
    <!-- SLF4J - False Positives -->
    <suppress>
        <notes><![CDATA[
        Suppress SLF4J false positives
        These are not applicable to our usage
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.slf4j/slf4j.*$</packageUrl>
        <cve>CVE-2018-8088</cve>
    </suppress>
    
    <!-- Apache Commons - False Positives -->
    <suppress>
        <notes><![CDATA[
        Suppress Apache Commons false positives
        These are not applicable to our usage patterns
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.apache\.commons/commons-.*$</packageUrl>
        <cve>CVE-2014-0114</cve>
    </suppress>
    
    <!-- JWT - False Positives -->
    <suppress>
        <notes><![CDATA[
        Suppress JWT library false positives
        These are not applicable to our JWT implementation
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/io\.jsonwebtoken/jjwt.*$</packageUrl>
        <cve>CVE-2019-10086</cve>
    </suppress>
    
    <!-- Micrometer - False Positives -->
    <suppress>
        <notes><![CDATA[
        Suppress Micrometer false positives
        These are monitoring libraries, not security vulnerabilities
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/io\.micrometer/micrometer.*$</packageUrl>
        <cve>CVE-2021-44228</cve>
    </suppress>
    
    <!-- Test Dependencies - All Scopes -->
    <suppress>
        <notes><![CDATA[
        Suppress all test dependencies
        These are not included in production builds
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/spring-boot-starter-test.*$</packageUrl>
        <scope>test</scope>
    </suppress>
    
    <suppress>
        <notes><![CDATA[
        Suppress Spring Security Test
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.security/spring-security-test.*$</packageUrl>
        <scope>test</scope>
    </suppress>
    
    <!-- Development Dependencies -->
    <suppress>
        <notes><![CDATA[
        Suppress development dependencies
        These are not included in production builds
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.projectlombok/lombok.*$</packageUrl>
        <scope>provided</scope>
    </suppress>
    
    <!-- Build Dependencies -->
    <suppress>
        <notes><![CDATA[
        Suppress build tool dependencies
        These are not included in runtime
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.apache\.maven\.plugins/.*$</packageUrl>
        <scope>provided</scope>
    </suppress>
    
</suppressions>