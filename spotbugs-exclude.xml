<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- Exclude false positives and acceptable patterns -->
    
    <!-- Exclude serialVersionUID warnings for DTOs -->
    <Match>
        <Class name="~.*\.dto\..*"/>
        <Bug pattern="SE_NO_SERIALVERSIONID"/>
    </Match>
    
    <!-- Exclude unused fields in configuration classes -->
    <Match>
        <Class name="~.*\.config\..*"/>
        <Bug pattern="UWF_UNWRITTEN_FIELD"/>
    </Match>
    
    <!-- Exclude logger field warnings -->
    <Match>
        <Bug pattern="UUF_UNUSED_FIELD"/>
        <Field name="log"/>
    </Match>
    
    <!-- Exclude Spring Boot main class warnings -->
    <Match>
        <Class name="~.*Application"/>
        <Bug pattern="UUF_UNUSED_FIELD"/>
    </Match>
    
    <!-- Exclude JPA entity warnings -->
    <Match>
        <Class name="~.*\.domain\..*"/>
        <Bug pattern="EI_EXPOSE_REP"/>
    </Match>
    
    <!-- Exclude test class warnings -->
    <Match>
        <Class name="~.*Test"/>
        <Bug pattern="UUF_UNUSED_FIELD"/>
    </Match>
    <Match>
        <Class name="~.*Tests"/>
        <Bug pattern="UUF_UNUSED_FIELD"/>
    </Match>
</FindBugsFilter>